<contentView id="my_cv" version="1.2">
  <contentViewModel>
    <title></title>
    <translateTitle>false</translateTitle>
    <showTitle>false</showTitle>
    <showFilterForm>true</showFilterForm>
    <showQuickFilterForm>false</showQuickFilterForm>
    <showFilterUnfolded>true</showFilterUnfolded>
    <showSyndicationLinks>false</showSyndicationLinks>
    <showPageSizeSelector>true</showPageSizeSelector>
    <showRefreshCommand>true</showRefreshCommand>
    <showCSVExport>true</showCSVExport>
    <showPDFExport>false</showPDFExport>
    <searchLayout class="simple-layout">
      <mode>edit</mode>
      <rows>
        <simple-layout-row>
          <widgets>
            <search-widget>
              <type>text</type>
              <label>Title</label>
              <helpLabel></helpLabel>
              <translated>false</translated>
              <properties class="linked-hash-map">
                <entry>
                  <string>edit</string>
                  <map/>
                </entry>
              </properties>
              <multiValued>false</multiValued>
              <readOnly>false</readOnly>
              <modeExpression></modeExpression>
              <colSpan>1</colSpan>
              <handleLabels>false</handleLabels>
              <schema>dublincore</schema>
              <schemaPrefix>dc</schemaPrefix>
              <fieldId>title</fieldId>
              <fieldType>string</fieldType>
              <searchOperator>FULLTEXT</searchOperator>
              <indexHint></indexHint>
              <analyzerHint></analyzerHint>
              <operatorHint></operatorHint>
            </search-widget>
          </widgets>
        </simple-layout-row>
        <simple-layout-row>
          <widgets>
            <search-widget>
              <type>text</type>
              <label>Full Text</label>
              <helpLabel></helpLabel>
              <translated>false</translated>
              <properties class="linked-hash-map">
                <entry>
                  <string>edit</string>
                  <map/>
                </entry>
              </properties>
              <multiValued>false</multiValued>
              <readOnly>false</readOnly>
              <modeExpression></modeExpression>
              <colSpan>1</colSpan>
              <handleLabels>false</handleLabels>
              <schema>system</schema>
              <schemaPrefix>ecm</schemaPrefix>
              <fieldId>fulltext</fieldId>
              <fieldType>string</fieldType>
              <searchOperator>FULLTEXT</searchOperator>
              <indexHint></indexHint>
              <analyzerHint></analyzerHint>
              <operatorHint></operatorHint>
            </search-widget>
          </widgets>
        </simple-layout-row>
        <simple-layout-row>
          <widgets>
            <null-widget>
              <colSpan>1</colSpan>
            </null-widget>
          </widgets>
        </simple-layout-row>
      </rows>
      <scaffolded>false</scaffolded>
    </searchLayout>
    <searchDocumentBinding></searchDocumentBinding>
    <fixedPart>ecm:parentId = ? AND ecm:mixinType != &apos;HiddenInNavigation&apos; AND ecm:isVersion = 0 AND ecm:isTrashed = 0</fixedPart>
    <queryParameters>
      <string>#{currentDocument.id}</string>
    </queryParameters>
    <quotePatternParameters>true</quotePatternParameters>
    <escapePatternParameters>true</escapePatternParameters>
    <defaultSortInfos/>
    <sortInfosBinding></sortInfosBinding>
    <sortable>true</sortable>
    <pageSize>10</pageSize>
    <pageSizeBinding></pageSizeBinding>
    <useGlobalPageSize>true</useGlobalPageSize>
    <resultLayoutReference>
      <enabled>true</enabled>
      <layoutName>search_listing_ajax</layoutName>
    </resultLayoutReference>
    <resultColumnsBinding></resultColumnsBinding>
    <defaultListingLayout class="simple-layout">
      <mode>listing</mode>
      <rows>
        <listing-layout-row>
          <widgets/>
          <translated>false</translated>
        </listing-layout-row>
        <listing-layout-row>
          <widgets/>
          <translated>false</translated>
        </listing-layout-row>
        <listing-layout-row>
          <widgets/>
          <translated>false</translated>
        </listing-layout-row>
        <listing-layout-row>
          <widgets/>
          <translated>false</translated>
        </listing-layout-row>
        <listing-layout-row>
          <widgets/>
          <translated>false</translated>
        </listing-layout-row>
      </rows>
    </defaultListingLayout>
    <additionalListingLayout class="simple-layout">
      <mode>listing</mode>
      <rows>
        <listing-layout-row>
          <widgets/>
          <translated>false</translated>
        </listing-layout-row>
        <listing-layout-row>
          <widgets/>
          <translated>false</translated>
        </listing-layout-row>
        <listing-layout-row>
          <widgets/>
          <translated>false</translated>
        </listing-layout-row>
        <listing-layout-row>
          <widgets/>
          <translated>false</translated>
        </listing-layout-row>
        <listing-layout-row>
          <widgets/>
          <translated>false</translated>
        </listing-layout-row>
      </rows>
    </additionalListingLayout>
    <useStandardListingLayouts>false</useStandardListingLayouts>
    <useStandardActions>true</useStandardActions>
    <useAdditionalActions>true</useAdditionalActions>
    <otherActionCategories/>
    <flags/>
    <emptySentence></emptySentence>
    <translateEmptySentence>false</translateEmptySentence>
    <cacheKey></cacheKey>
    <selectionList></selectionList>
    <maxResults></maxResults>
    <refreshEvents>
      <string>documentChanged</string>
      <string>documentChildrenChanged</string>
    </refreshEvents>
    <resetEvents>
      <string>documentChanged</string>
      <string>documentChildrenChanged</string>
    </resetEvents>
    <pageProviderClass></pageProviderClass>
    <pageProviderProperties/>
    <useElasticsearchIndex>true</useElasticsearchIndex>
    <actionIndex>0</actionIndex>
    <trackUsage>true</trackUsage>
    <columnEdition>true</columnEdition>
    <showSpreadsheet>true</showSpreadsheet>
    <waitForExecution>false</waitForExecution>
    <waitForExecutionSentence></waitForExecutionSentence>
  </contentViewModel>
</contentView>